<h1>All Products</h1>
<% @products.each do |product| %>
    <h2><a href="/products/<%=product.id %>"><%= product.name %></a></h2>
    <h3><%= product.price %></h3>
    <h4><%= product.image %></h4>
    <h5><%= product.description %></h5>
<% end %>

<button class="btn btn-info">Is Bootstrap Working?</button>